LOAD DATA LOCAL INFILE 'datasets/A' INTO TABLE Ap
    FIELDS TERMINATED BY ',' ENCLOSED BY '"' 
    LINES TERMINATED BY '\r\n';
LOAD DATA LOCAL INFILE 'datasets/B' INTO TABLE Bp
    FIELDS TERMINATED BY ',' ENCLOSED BY '"' 
    LINES TERMINATED BY '\r\n';
LOAD DATA LOCAL INFILE 'datasets/C' INTO TABLE Cp
    FIELDS TERMINATED BY ',' ENCLOSED BY '"' 
    LINES TERMINATED BY '\r\n';
