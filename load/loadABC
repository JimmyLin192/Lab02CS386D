LOAD DATA LOCAL INFILE 'datasets/A' INTO TABLE A
    FIELDS TERMINATED BY ',' ENCLOSED BY '"' 
    LINES TERMINATED BY '\r\n';
LOAD DATA LOCAL INFILE 'datasets/B' INTO TABLE B 
    FIELDS TERMINATED BY ',' ENCLOSED BY '"' 
    LINES TERMINATED BY '\r\n';
LOAD DATA LOCAL INFILE 'datasets/C' INTO TABLE C
    FIELDS TERMINATED BY ',' ENCLOSED BY '"' 
    LINES TERMINATED BY '\r\n';
