EXPLAIN SELECT * FROM A, C, B WHERE C.ot = B.ht and B.ht = A.ht;
EXPLAIN SELECT * FROM B, A, C WHERE C.ot = B.ht and B.ht = A.ht;
EXPLAIN SELECT * FROM B, C, A WHERE C.ot = B.ht and B.ht = A.ht;
EXPLAIN SELECT * FROM C, A, B WHERE C.ot = B.ht and B.ht = A.ht;
